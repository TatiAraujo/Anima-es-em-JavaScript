var olhosTam = 20;
var denteTam = 20;
ellipse(150, 70, 60, 120);  // left ear:60,120
ellipse(240, 70, 60, 120);  // right ear

ellipse(200, 170, 150, 150);    // face

fill(0, 0, 0);
ellipse(170, 150, olhosTam, olhosTam);  // left eye: 10,10
ellipse(230, 150, olhosTam, olhosTam);  // right eye

line(150, 200, 250, 200);   // mouth

noFill();
rect(185, 200, 15, denteTam); // left tooth
rect(200, 200, 15, denteTam); // right tooth

